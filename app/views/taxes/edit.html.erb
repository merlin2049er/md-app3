<% @user = current_user %>
<% if user_signed_in? && @user.admin %>


<h1>Editing Tax</h1>

<%= render 'form', tax: @tax %>

<%= link_to 'Show', @tax %> |
<%= link_to 'Back', taxes_path %>

<% else %>
  <div class="alert alert-warning" role="alert"> You must be signed in to edit the taxes.</div>
<% end %>
