<!-- Find me in app/views/taxes/_form -->

<%= simple_form_for(@tax) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :enabled %>
    <%= f.input :prov_id %>
    <%= f.input :tax_rate %>
    <%= f.input :comment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<!-- end -->
